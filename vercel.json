{
    "version": 2,
    "functions": {
      "index.js": {
        "runtime": "nodejs18.x",
        "maxDuration": 10
      }
    },
    "routes": [
      { "src": "/.*", "dest": "/" }
    ],
    "headers": [
      {
        "source": "/webhook",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" },
          { "key": "Access-Control-Allow-Methods", "value": "GET,POST,OPTIONS" },
          { "key": "Content-Type", "value": "application/json" }
        ]
      }
    ]
  }
  